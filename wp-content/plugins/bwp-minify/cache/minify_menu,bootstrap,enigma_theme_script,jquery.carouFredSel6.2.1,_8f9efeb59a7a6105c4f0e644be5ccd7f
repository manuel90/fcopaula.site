{var s=' ('+d.selector+')';d=d.debug;}
else
{var s='';}
if(!d)
{return false;}
if(is_string(m))
{m='wl_caroufredsel'+s+': '+m;}
else
{m=['wl_caroufredsel'+s+':',m];}
window.console.log(m);}
return false;}
